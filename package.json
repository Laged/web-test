{
  "name": "web-test",
  "version": "1.0.0",
  "description": "Testing modern web stuff",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "debug": "nodemon src/index.js --exec babel-node",
    "test": "yarn build && mocha --compilers js:babel-core/register",
    "test-cov": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "watch": "babel src -d dist --source-maps --watch",
    "start": "yarn build && yarn serve",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "bin": {
    "joke": ".src/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.1.4",
    "babel-plugin-transform-regenerator": "^6.1.4",
    "babel-polyfill": "^6.1.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.15.0",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.11.0",
    "request": "^2.65.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "flow-bin": "^0.38.0",
    "isparta": "^4.0.0",
    "mocha": "^3.2.0"
  }
}
